target 'Developer' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Developer

  target 'DeveloperTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DeveloperUITests' do
    inherit! :search_paths
    # Pods for testing
  end


