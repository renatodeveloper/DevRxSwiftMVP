    # Pod for Alamofire
      pod 'Alamofire' , :git => 'https://github.com/Alamofire/Alamofire.git' , :branch =>  'alamofire5'
   
    # Pods for RxSwift
      pod 'RxSwift', '~> 4.0'
      pod 'RxCocoa', '~> 4.0'

    # Pod for SwiftJSON
      pod 'SwiftyJSON', '~> 4.0'
end
